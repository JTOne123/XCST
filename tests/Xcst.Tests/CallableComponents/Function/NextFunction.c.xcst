   <c:import href='_NextFunction.xcst'/>

   <c:function name='foo' as='string' expand-text='yes'>
      <c:return>{__xcst.next_function()}baz</c:return>
   </c:function>

   <c:template name='c:initial-template'>
      <foo>
         <c:value-of value='foo()'/>
