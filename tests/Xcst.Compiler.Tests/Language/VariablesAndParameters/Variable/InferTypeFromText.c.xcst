<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:variable name='a'>
         <c:text>foo</c:text>
      </c:variable>
      <foo>
         <c:value-of value='a is string'/>
