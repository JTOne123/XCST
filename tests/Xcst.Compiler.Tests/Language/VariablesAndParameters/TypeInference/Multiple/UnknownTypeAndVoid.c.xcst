<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:variable name='a'>
         <c:object value='"A"'/>
         <c:assert test='true'/>
      </c:variable>
      <c:void value='Assert.AreEqual(typeof(object[]), CompileType(a))'/>
      <c:variable name='b'>
         <c:object value='"A"'/>
         <c:object value='"1"'/>
         <c:assert test='true'/>
      </c:variable>
      <c:void value='Assert.AreEqual(typeof(object[]), CompileType(b))'/>
   </c:template>

</c:module>