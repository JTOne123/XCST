<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:use-functions in='Microsoft.VisualStudio.TestTools.UnitTesting'/>

   <c:template name='c:initial-template'>
      <c:variable name='a'/>
      <c:set member='a'/>
      <c:void value='Assert.IsNull(a)'/>
      <c:variable name='b' as='int'/>
      <c:set member='b' as='int'/>
      <c:void value='Assert.AreEqual(b, default(int))'/>
   </c:template>

</c:module>